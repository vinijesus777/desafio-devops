{
  "title": "Desafio DevOps \u2014 Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "Requests (sum, 5m)",
      "id": 1,
      "targets": [
        {
          "expr": "sum(rate(app_requests_total[5m]))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Requests por endpoint (rate 5m)",
      "id": 2,
      "targets": [
        {
          "expr": "sum by (service,endpoint) (rate(app_requests_total[5m]))"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Cache Hit Ratio (5m)",
      "id": 3,
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      },
      "targets": [
        {
          "expr": "sum(rate(app_cache_hits_total[5m])) / (sum(rate(app_cache_hits_total[5m])) + sum(rate(app_cache_misses_total[5m])))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Status (up) por job",
      "id": 4,
      "targets": [
        {
          "expr": "up{job=~\"app1|app2|prometheus\"}"
        }
      ]
    }
  ]
}