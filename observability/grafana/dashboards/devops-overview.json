{
  "id": null,
  "uid": "devops-overview",
  "title": "Desafio DevOps \u2014 Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "Requests (sum)",
      "targets": [
        {
          "expr": "sum(app_requests_total)",
          "legendFormat": "requests"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      }
    },
    {
      "type": "stat",
      "title": "Cache Hit Ratio (5m)",
      "targets": [
        {
          "expr": "sum(rate(app_cache_hits_total[5m])) / (sum(rate(app_cache_hits_total[5m])) + sum(rate(app_cache_misses_total[5m])))",
          "legendFormat": "hit_ratio"
        }
      ],
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      }
    },
    {
      "type": "timeseries",
      "title": "Requests by endpoint",
      "targets": [
        {
          "expr": "sum by (service, endpoint) (rate(app_requests_total[5m]))",
          "legendFormat": "{{service}} {{endpoint}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Latency p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, service, endpoint) (rate(app_request_latency_seconds_bucket[5m])))",
          "legendFormat": "{{service}} {{endpoint}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 8
      }
    }
  ]
}